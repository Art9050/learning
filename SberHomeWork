---CAR-
CREATE TABLE de11an.kart_CAR(
	VinNumber VARCHAR(50) PRIMARY KEY, 
	ProductionDate DATE,
	VehileSaleDate DATE,
	VehileType VARCHAR(5) REFERENCES de11an.kart_VehileType(id),
	Warranty VARCHAR(3) REFERENCES de11an.kart_Warranty(Type)
	);

insert into de11an.kart_CAR (VinNumber, ProductionDate,	VehileSaleDate,	VehileType, Warranty) values('JFRGFL130GY091899', '2016-06-28', '2016-07-04', 't2', 'a');
