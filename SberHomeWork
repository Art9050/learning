---PartReason-
create table de11an.kart_PartReason(
	PartCode varchar(50) primary key ,
	PartName varchar(100) not null,
	Price NUMERIC(18,5) not null,
	Manufacturer char(1) not null REFERENCES de11an.kart_Manufacturer(Code)
	);

